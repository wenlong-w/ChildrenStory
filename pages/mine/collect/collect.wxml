<import src="../../template/audio/audio.wxml"/>
<view class="container" style='display:block'>
  <view wx:for="{{audioList}}" wx:key="{{id}}" class='audio-template'>
    <template is="audioItem" data="{{...item}}"/>
  </view>
</view>
<view class='low-right-back' bindtap='goBack'>
  <image src='../../image/goBack.png' style='width:40px;height:40px'></image>
</view>